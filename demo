import requests
import csv
import pandas as pd
from bs4 import BeautifulSoup
import pandas as pd

clinics = []
address = []
telephone = []
for i in range(0,14): # Page counter
    url = "https://map.baidu.com/search/%E5%8C%97%E4%BA%AC%E5%B8%82%20%E8%A5%BF%E5%9F%8E%E5%8C%BA%20%E8%AF%8A%E6%89%80/@12953202.35,4827380.88,13z?querytype=s&da_src=shareurl&wd=%E5%8C%97%E4%BA%AC%E5%B8%82%20%E8%A5%BF%E5%9F%8E%E5%8C%BA%20%E8%AF%8A%E6%89%80&c=131&src=0&pn="+ str(i) +"&sug=0&l=12&b=(12910068,4804022.5;13008372,4846646.5)&from=webmap&biz_forward=%7B%22scaler%22:2,%22styles%22:%22pl%22%7D&&device_ratio=2"
    quote_page = requests.get(url) 
    # print("\n***** Page " + str(i) +" in action *****")
    soup = BeautifulSoup(quote_page.content,'html.parser')
    for item in soup.findAll("div", attrs={"class":"ml_30 mr_90"}):
        page_no = str(i)           
        name = item.find("div",attrs={"class":"row"})
        addr = item.find("div",attrs={"class":"row addr"})
        tel = item.find("div",attrs={"class":"row tel"})     
        if name != None:
            clinics.append(name.text)
        else:
            clinics.append(None) 
        if addr != None:
            address.append(addr.text)
        else:
            address.append(None) 
        if tel != None: 
            telephone.append(tel.text)
        else:
            telephone.append(None) 
                
print(clinics)
#df = pd.DataFrame({"Clinic Name":clinics,"Address":address, "Tel":telephone})           
